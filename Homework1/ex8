N = int(input())

l = [0] * (N+1)             # массив начинается с 0, а кончается N, причем в i-м элементе лежит число i
for i in range (N+1):
    l[i] = i
i = 0

if N >= 1:                  # обрабатываем массив
    l[1] = 0                # 1 не является простым числом

a = 0
for i in range (N+1):
    if l[i] > 0:
        print(l[i])
        a = 2 * l[i]
        while a <= N:
            l[a] = 0
            a = a + l[i]
